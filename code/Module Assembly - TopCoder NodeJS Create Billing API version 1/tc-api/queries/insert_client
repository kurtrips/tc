VALUES (@clientId@, '@clientName@', @companyId@, @paymentTermId@, @status@, @salestax@, CURRENT, CURRENT + @durationInYears@ UNITS YEAR, CURRENT, @userId@, CURRENT, @userId@, '@customerNumber@')